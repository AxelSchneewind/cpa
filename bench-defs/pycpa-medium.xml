<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="pycpa">

    <propertyfile>../benchmarks/properties/unreach-call.prp</propertyfile>
  

    <rundefinition name="Value Analysis">
      <option name="--config">ValueAnalysis</option>
    </rundefinition>

    <rundefinition name="Formula Analysis">
      <option name="--config">FormulaAnalysis</option>
    </rundefinition>

    <rundefinition name="Predicate Analysis">
      <option name="--config">PredicateAnalysis</option>
    </rundefinition>

    <rundefinition name="Predicate Analysis ABE lf">
      <option name="--config">PredicateAnalysisABElf</option>
    </rundefinition>

    <rundefinition name="Predicate Analysis CEGAR">
      <option name="--config">PredicateAnalysisCEGAR</option>
    </rundefinition>

    <rundefinition name="Predicate Analysis CEGAR ABElf">
      <option name="--config">PredicateAnalysisCEGARABElf</option>
    </rundefinition>

  
    <tasks name="demo">
      <includesfile>sets/demo.set</includesfile>
    </tasks>


    <tasks name='ReachSafety-BitVectors'>
        <includesfile>sets/ReachSafety-BitVectors.set</includesfile></tasks>
    <tasks name='ReachSafety-Combinations'>
        <includesfile>sets/ReachSafety-Combinations.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-ControlFlow'>
        <includesfile>sets/ReachSafety-ControlFlow.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-ECA'>
        <includesfile>sets/ReachSafety-ECA.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-Hardness'>
        <includesfile>sets/ReachSafety-Hardness.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-Hardware'>
        <includesfile>sets/ReachSafety-Hardware.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-Sequentialized'>
        <includesfile>sets/ReachSafety-Sequentialized.set</includesfile>
    </tasks>
    <tasks name='ReachSafety-XCSP'>
        <includesfile>sets/ReachSafety-XCSP.set</includesfile>
    </tasks>
  

</benchmark>
